			"RowStyle": "row sysListContent",
			"Navigation": {
				"Type": "Page.Index"
			},
			"Columns": {
				"country": {
					"HeaderTextID": "TXT.TABLE.HEADER.CONTACT.NEW.PHONE.COUNTRY",
