				"Remove": {
					"TextID": "TXT.CONTEXTMENU.REMOVE",
					"Icon": "Delete",
					"InternalFunction": "remove",
					"InternalRemoveItemBy": "RowIndex"
				},
				"Reset": {
					"TextID": "TXT.CONTEXTMENU.RESET",
					"Icon": "Delete",
					"InternalFunction": "reset"
				}
			}
