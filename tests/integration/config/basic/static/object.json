					"HeaderStyle": "col-md-3"
				},
				"areacode": {
					"HeaderTextID": "TXT.TABLE.HEADER.CONTACT.NEW.PHONE.AREACODE",
					"sortable": true,
					"HeaderStyle": "col-md-3"
				},
				"number": {
					"HeaderTextID": "TXT.TABLE.HEADER.CONTACT.NEW.PHONE.NUMBER",
					"sortable": true,
					"HeaderStyle": "col-md-6"
				}
			},
			"ElementsEnclosedByDivStyle": [
				"col-md-3",
				"col-md-3",
				"col-md-6"
			],
