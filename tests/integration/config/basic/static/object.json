		}
	},
	"FormFieldList1": {
		"Type": "FormfieldList",
		"Attributes": {
			"Style": "FormfieldList",
			"Formfields": [
				"FormField1Label",
				"FormField1",
				"FormFieldPulldown1",
				"FormFieldDynPulldown1"
			]
		}
	},
	"FormField1Label": {
		"Type": "FormField",
		"Attributes": {
			"Type": "label",
			"DisplayText": "Form Field 1",
			"Style": "required sysHeadline",
			"LabelFor": "FormField1"
		}
	},
	"FormField1": {
		"Type": "FormField",
		"Attributes": {
			"Type": "text",
			"ValidateRef": "DefaultString",
			"ValidateNullable": true,
			"Style": "FormFieldBorder",
			"StyleOnFocus": "FormFieldBorderOnFocus",
			"StyleValidateOk": "FormFieldBorderValidateOk",
			"StyleValidateFail": "FormFieldBorderValidateFail"
		}
	},
	"FormFieldPulldown1": {
		"Type": "FormField",
		"Attributes": {
			"Type": "pulldown",
			"Style": "FormFieldBorderPulldown",
			"StyleOnFocus": "StyleOnFocus",
			"AddNoneItem": true,
			"Options": [
				{
					"TextID": "TXT.ID.1",
					"Value": "10"
				},
				{
					"TextID": "TXT.ID.2",
					"Value": "20",
					"Default": true
				}
			]
		}
	},
	"FormFieldDynPulldown1": {
		"Type": "FormField",
		"Attributes": {
			"Type": "dynpulldown",
			"AddNoneItem": true,
