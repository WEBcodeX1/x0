			"ServiceID": "test_dynpulldown1"
		}
	},
	"ServiceConnector1": {
		"Type": "ServiceConnector",
		"Attributes": {
			"OnEvent": {
				"Events": [ "InitSystem" ],
				"ServiceCall": "python/getTestList1Data.py"
			}
		}
	},
	"List1": {
		"Type": "List",
		"Attributes": {
			"RowCount": 10,
			"Style": "sysList",
