{
    "project": {
        "name": "x0-base",
        "id": "x0",
        "git-repo": "x0"
    },
    "installer": {
        "type": "x0",
        "subtype": "minikube",
        "os:": "mswindows",
        "os_version": 11,
	"offline_install": false,
	"minikube_driver": "hyperv"
    },
    "database": {
        "name": "x0",
        "su_password": "changeme",
        "repl_password": "changeme",
        "x0_password": "changeme",
        "size": "50Mi"
    },
    "test": {
        "selenium": {
            "run_servers": 1,
            "run_images": [
                "chrome:latest"
            ]
        },
        "testenvs": [ "test" ]
    },
    "env_list": [ "test" ],
    "environments": {
        "test": {
            "kubernetes": {
                "deployment": {
                    "image": "x0-app",
                    "replicas": 1,
                    "cpu": "200m",
                    "memory": "512Mi",
                    "autoscale": false
                },
                "namespace": "x0-app",
                "app_templates": [ "kubegres" ]
            }
        }
    },
    "vhosts": {
        "x0-app": {
            "apps": [ "x0" ],
            "env": {
                "test": {
                    "dns": {
                        "hostname": "x0-app",
                        "domain": "x0.localnet"
                    },
                    "ip": {
                        "v4": {
                            "dns_register": true,
                            "dns_register_type": "x0-net-management"
                        }
                    }
                }
            }
        }
    }
}
